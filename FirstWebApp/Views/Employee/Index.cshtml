@model List<Employee>

<h3>Employees</h3>

<table class="table">
    <tr>
        <th>EmployeeID</th>
        <th>Name</th>
        <th>Last Name</th>
        <th>Date Of Birth</th>
        <th>Email Address</th>
        <th>Phone Number</th>
    </tr>

    @foreach (Employee emp in Model)
    {
        <tr>

            <td>@emp.EmployeeID</td>
            <td>@emp.FirstName</td>
            <td>@emp.LastName</td>
            <td>@emp.DateOfBirth</td>
            <td>@emp.EmailAddress</td>
            <td>@emp.PhoneNumber</td>

        </tr>
    }

</table>